
CREATE TABLE `trade_bill` (
  `TRANSACTION_ID` bigint(20) NOT NULL AUTO_INCREMENT COMMENT '交易ID',
  `TRADE_ID` bigint(20) DEFAULT NULL COMMENT '贸易ID',
  `VERSION` bigint(20) DEFAULT NULL COMMENT 'version',
  `SECURITY_CODE` varchar(25) DEFAULT NULL COMMENT 'SecurityCode',
  `QUANTITY` int(11) DEFAULT NULL COMMENT '数量',
  `ACTION_STATUS` varchar(50) DEFAULT NULL COMMENT 'update/insert/cancel',
  `TRADE_TYPE` varchar(50) DEFAULT NULL COMMENT 'buy/sell',
  `GMT_CREATED` datetime DEFAULT NULL COMMENT '创建时间',
  `GMT_UPDATE` datetime DEFAULT NULL COMMENT '修改时间',
  PRIMARY KEY (`TRANSACTION_ID`)
) ENGINE=InnoDB AUTO_INCREMENT=620 DEFAULT CHARSET=utf8;


CREATE TABLE `position` (
  `ID` bigint(20) NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `TYPE` bigint(20) DEFAULT NULL COMMENT 'rel/itc/itc',
  `AMOUNT` bigint(20) DEFAULT NULL COMMENT 'amount',
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB AUTO_INCREMENT=620 DEFAULT CHARSET=utf8;
